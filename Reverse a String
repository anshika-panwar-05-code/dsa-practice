import java.util.*;
class Solution {
    public static String reverseString(String s) {
        
       char[] ch = s.toCharArray();
       
      int i=0;
      int j=ch.length-1;
      
      
      while(i<j){
          char c=ch[i];
          ch[i]=ch[j];
          ch[j]=c;
          i++;
          j--;
      }
      
      return String.valueOf(ch);
        
    }
}
