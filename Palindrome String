class Solution {
    boolean isPalindrome(String s) {
        // code here
        char[] ch=s.toCharArray();
        
        int i=0;
        int j=ch.length-1;
        
        while(i<j){
            if(ch[i]!=ch[j]){
                return false;
            }
            i++;
            j--;
        }
        
       return true;
      
    }
}
