import java.util.*;
class Main {
    public static void main(String[] args) {
        int[] arr={12, 1, 2, 3, 0, 11, 4};
        List<Integer> res = new ArrayList<>();
    
      for(int i=0;i<arr.length;i++){
           int count=0;
          for(int j=i+1;j<arr.length;j++){
              if(arr[j]<arr[i]){
                  count++;
                  
              }
          }
              res.add(count);
          }
      
       System.out.println(res);
    }
}
