
class Main {
    public static void main(String[] args) {
      int[] arr= {1, 5, 5, 6, 6, 7};
      int n=arr.length;
      
      for(int i=n-1;i>0;i--){
          if(arr[i]==arr[i-1]){
              System.out.println(arr[i]+" "+ i);
              return;
            //   Java में return का मतलब है: method (function) को वहीं पर खत्म कर देना।
             
          }
      }
     System.out.println("not found");
     
      
    }
}
