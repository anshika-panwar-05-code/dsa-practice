import java.util.ArrayList;
//arraylist use h isliye size liya h aur arr.get(i) for get the element.
class Solution {
    public int search(int k, ArrayList<Integer> arr) {
        // code here
        for(int i=0;i<arr.size();i++){
            if(arr.get(i)==k){
                return i+1;
               
            }
        }
        return -1;
    }
}
